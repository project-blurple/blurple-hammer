{
  "name": "blurple-hammer",
  "main": "build",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "start": "node .",
    "watch": "tsc -w",
    "docker": "npm run docker:build && npm run docker:up",
    "docker:build": "docker-compose build",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs --tail=500 -f",
    "docker:start": "npm run docker:up",
    "docker:stop": "npm run docker:down",
    "docker:up": "docker-compose up -d"
  },
  "dependencies": {
    "@typegoose/typegoose": "9.11.2",
    "bufferutil": "4.0.7",
    "cookie-parser": "1.4.6",
    "dedent": "0.7.0",
    "discord-oauth2": "2.10.1",
    "discord.js": "14.7.1",
    "dotenv": "16.0.3",
    "erlpack": "github:discord/erlpack",
    "express": "4.18.2",
    "express-rate-limit": "6.7.0",
    "helmet": "6.0.1",
    "jsonwebtoken": "8.5.1",
    "match-sorter": "6.3.1",
    "mongoose": "6.5.5",
    "morgan": "1.10.0",
    "nodemailer": "6.8.0",
    "superagent": "8.0.5",
    "svg-png-converter": "0.0.9",
    "text-to-svg": "3.1.5",
    "utf-8-validate": "5.0.10",
    "winston": "3.8.2",
    "winston-daily-rotate-file": "4.7.1",
    "zlib-sync": "0.1.7"
