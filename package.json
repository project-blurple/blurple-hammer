{
  "name": "blurple-hammer",
  "main": "build",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc -w",
    "docker": "npm run docker:build && npm run docker:up",
    "docker:build": "docker-compose --project-directory . build",
    "docker:down": "docker-compose --project-directory . down",
    "docker:logs": "docker-compose --project-directory . logs --tail=500 -f",
    "docker:start": "npm run docker:up",
    "docker:stop": "npm run docker:down",
    "docker:up": "docker-compose --project-directory . up -d",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "start": "node .",
    "test": "jest"
  },
  "dependencies": {
    "@sapphire/type": "2.3.0",
    "@typegoose/typegoose": "10.3.2",
    "bufferutil": "4.0.7",
    "cookie-parser": "1.4.6",
    "decompress": "4.2.1",
    "dedent": "0.7.0",
    "discord-oauth2": "2.11.0",
    "discord.js": "14.7.1",
    "dotenv": "16.0.3",
    "erlpack": "github:discord/erlpack",
    "express": "4.18.2",
    "express-rate-limit": "6.7.0",
    "fs-extra": "11.1.0",
    "helmet": "6.0.1",
    "jsonwebtoken": "9.0.0",
    "match-sorter": "6.3.1",
    "mongoose": "6.10.3",
    "morgan": "1.10.0",
    "nodemailer": "6.9.1",
    "superagent": "8.0.9",
    "svg-png-converter": "0.0.9",
    "svg2img": "1.0.0-beta.2",
    "text-to-svg": "3.1.5",
    "utf-8-validate": "5.0.10",
    "winston": "3.8.2",
    "winston-daily-rotate-file": "4.7.1",
    "zlib-sync": "0.1.8"
  },
  "devDependencies": {
    "@tsconfig/node18-strictest": "1.0.0",
    "@types/cookie-parser": "1.4.3",
    "@types/decompress": "4.2.4",
    "@types/dedent": "0.7.0",
    "@types/express": "4.17.17",
    "@types/fs-extra": "11.0.1",
    "@types/jest": "29.4.2",
    "@types/jsonwebtoken": "9.0.1",
    "@types/morgan": "1.9.4",
    "@types/node": "18.15.3",
    "@types/nodemailer": "6.4.7",
    "@types/superagent": "4.1.16",
    "@types/text-to-svg": "3.1.1",
    "eslint": "8.36.0",
    "eslint-config-promise": "github:promise/eslint-config",
    "jest": "29.5.0",
    "ms": "3.0.0-canary.1",
    "ts-jest": "29.0.5",
    "typescript": "4.9.5"
  },
  "eslintConfig": {
    "extends": "promise"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "<rootDir>/build/",
      "<rootDir>/node_modules/"
    ]
  }
}
