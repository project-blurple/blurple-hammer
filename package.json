    "@types/dedent": "0.7.0",
    "@types/express": "4.17.14",
    "@types/jsonwebtoken": "8.5.9",
    "@types/morgan": "1.9.3",
    "@types/node": "18.11.11",
    "@types/nodemailer": "6.4.6",
    "@types/superagent": "4.1.16",
    "@types/text-to-svg": "3.1.1",
    "eslint": "8.29.0",
    "eslint-config-promise": "github:promise/eslint-config",
    "ms": "3.0.0-canary.1",
    "typescript": "4.6.4"
  },
  "eslintConfig": {
    "extends": "promise"
  }
